package kr.brains;

// 하나의 파일에는 public class가 하나만 존재함
// 파일 이름은 public class 가 있는 경우 public class 이름임
// public class 가 없는 경우 이름은 선택가능함.
public class CalculatorExample {
	public static void main(String[] args) {
		// 기본 생성자(매개변수가 없는) 호출하여 객체를 생성
		 // calculator : 객체 참조 변수
		NewCalculator calculator = new NewCalculator();
		System.out.println(calculator.calc());
		calculator = new NewCalculator(23, "*", 40);
		System.out.println(calculator.calc());
		calculator = new NewCalculator(40, "/", 40);
		System.out.println(calculator.calc());
		calculator = new NewCalculator(40, "+", 40);
		System.out.println(calculator.calc());
	}
}
