		try {
			clazz1 = Class.forName("kr.brains.api.Car");
		} catch (ClassNotFoundException e)
		
		//Class clazz1 = Class.forName("kr.brains.api.Car");
		//System.out.println(clazz1.getPackageName());
	}
}
